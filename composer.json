{
    "name": "rakhasa/lararytr",
    "description": "This is my package lararytr",
    "keywords": [
        "rakhasa",
        "laravel",
        "lararytr"
    ],
    "homepage": "https://github.com/rakhasa/lararytr",
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmat Arnal Kastana",
            "email": "arnal@rakhasa.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.14.0",
        "illuminate/contracts": "^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Rakhasa\\Lararytr\\": "src",
            "Rakhasa\\Lararytr\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rakhasa\\Lararytr\\Tests\\": "tests"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rakhasa\\Lararytr\\LararytrServiceProvider"
            ],
            "aliases": {
                "Lararytr": "Rakhasa\\Lararytr\\Facades\\Lararytr"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}